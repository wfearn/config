#files
snippet withr
	with open(${1}, 'r') as f:
		${0}

snippet withw
	with open(${1}, 'w') as f:
		${0}

# for loops
snippet fore
	for i, ${1} in enumerate(${2}):
		${0}

snippet fordi
	for k, v in ${1}.items():
		${0}

# class template
snippet cl
	class ${1}(${2}):
		def __init__(self):
			pass

# main
snippet mainf
	if __name__ == '__main__':
		${1}

# functions
snippet fnc
	def ${1}(${2}):
		pass

snippet dnr
	def __${1}__(${2}):
		pass
